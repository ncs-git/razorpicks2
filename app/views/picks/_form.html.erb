<%= simple_form_for(@pick) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :home_score %>
    <%= f.input :away_score %>
    <%= f.association :user %>
    <%= f.association :week %>
    <%= f.association :game %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
