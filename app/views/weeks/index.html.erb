<h1>Listing weeks</h1>

<table>
  <thead>
    <tr>
      <th>Start</th>
      <th>End</th>
      <th>Due by</th>
      <th>Name</th>
      <th>Current</th>
      <th>Completed</th>
      <th>Locked</th>
      <th>Season</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weeks.each do |week| %>
      <tr>
        <td><%= week.start %></td>
        <td><%= week.end %></td>
        <td><%= week.due_by %></td>
        <td><%= week.name %></td>
        <td><%= week.current %></td>
        <td><%= week.completed %></td>
        <td><%= week.locked %></td>
        <td><%= week.season %></td>
        <td><%= link_to 'Show', week %></td>
        <td><%= link_to 'Edit', edit_week_path(week) %></td>
        <td><%= link_to 'Destroy', week, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Week', new_week_path %>
