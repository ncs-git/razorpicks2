<%= simple_form_for(@team) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :mascot %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :wins %>
    <%= f.input :loses %>
    <%= f.input :url %>
    <%= f.input :nickname %>
    <%= f.association :conference %>
    <%= f.association :pick %>
    <%= f.association :game %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
