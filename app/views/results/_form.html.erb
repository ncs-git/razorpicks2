<%= simple_form_for(@result) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :wins %>
    <%= f.input :loses %>
    <%= f.input :tiebreak_score %>
    <%= f.association :week %>
    <%= f.association :season %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
